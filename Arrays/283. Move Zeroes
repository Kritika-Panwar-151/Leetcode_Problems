class Solution {
    public void moveZeroes(int[] nums) 
    {
        /*
        int c[]=new int[nums.length];
        int j=-1;
        int n=nums.length;
        for(int i=0;i<nums.length;i++)
        {
            if(nums[i]!=0)
            {
                c[++j]=nums[i];
            }
            else
            {
                c[--n]=nums[i];
            }
        }
        for(int i=0;i<nums.length;i++)
        {
            nums[i]=c[i];
        }
        */
        int j=-1;
        for(int i=0;i<nums.length;i++)
        {
            if(nums[i]!=0)
            {
                nums[++j]=nums[i];
            }
            
        }
        for(int i=j+1;i<nums.length;i++)
        {
            nums[i]=0;
        }
    }
}