class Solution {
    public int pivotIndex(int[] nums) {
        int n=nums.length;
        int f=0;
        int e=0;
        for(int i:nums) e+=i;
        for(int i=0;i<n;i++)
        {
            e-=nums[i];
            if(f==e)
            {
                return i;
            }
            f+=nums[i];
            
            
        }
        return -1;
    }
}